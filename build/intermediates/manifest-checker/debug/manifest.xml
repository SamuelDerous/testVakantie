<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="be.zenodotus.testVakantie"
    android:versionCode="2"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="25" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <meta-data
        android:name="android.support.VERSION"
        android:value="25.3.1" />

    <application
        android:allowBackup="true"
        android:debuggable="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:testOnly="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="be.zenodotus.jvakantie.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="be.zenodotus.jvakantie.AddActivity"
            android:label="@string/addVerlof" >
        </activity>
        <activity
            android:name="be.zenodotus.jvakantie.AddSoort"
            android:label="@string/toevoegen" >
        </activity>
        <activity
            android:name="be.zenodotus.jvakantie.SoortenActivity"
            android:label="@string/soorten" >
        </activity>
        <activity
            android:name="be.zenodotus.jvakantie.BerekeningenActivity"
            android:label="@string/berekeningen" >
        </activity>
        <activity
            android:name="be.zenodotus.jvakantie.SettingsActivity"
            android:label="@string/settings" >
        </activity>

        <meta-data
            android:name="android.support.VERSION"
            android:value="25.4.0" />

        <provider
            android:name="com.android.tools.ir.server.InstantRunContentProvider"
            android:authorities="be.zenodotus.testVakantie.com.android.tools.ir.server.InstantRunContentProvider"
            android:multiprocess="true" />
    </application>

</manifest>